<!-- views/pages/about.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <% include ../partials/header %>
</header>

<main>
    <div class="row">
        <div class="col-sm-8">

            <div class="jumbotron">
                <h1>This is a list</h1>
                <p>Maybe you can download it in future</p>
            </div>

        </div>
        <div class="col-sm-4">

            <div class="well">
                <h3>List of item in Azure Blob!</h3>
                <% list.forEach(function(item) { %>
                <li><%= item.name %>
                <% }); %>
            </div>

        </div>
    </div>
</main>

<footer>
    <% include ../partials/footer %>
</footer>

</body>
</html>